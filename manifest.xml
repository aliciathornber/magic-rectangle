<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">

  <!-- REQUIRED: child elements in the schema-defined order -->
  <Id>7f7f6d3e-3c0a-4b1e-9e86-2c8f2d9f8a71</Id> <!-- replace with your GUID -->
  <Version>1.0.0.1</Version>
  <ProviderName>Rounded Corner Lock</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Magic Rectangle"/>
  <Description DefaultValue="Apply consistent sizing to rounded rectangle corners all at once."/>
  <IconUrl DefaultValue="https://aliciathornber.github.io/magic-rectangle/assets/icon-32.png"/>
  <!-- Optional high-res icon:
  <HighResolutionIconUrl DefaultValue="https://aliciathornber.github.io/magic-rectangle/assets/icon-64.png"/>
  -->
  <SupportUrl DefaultValue="https://github.com/aliciathornber/magic-rectangle"/>

  <AppDomains>
    <AppDomain>https://aliciathornber.github.io</AppDomain>
    <AppDomain>https://appsforoffice.microsoft.com</AppDomain>
  </AppDomains>

  <!-- PowerPoint host name: Presentation -->
  <Hosts>
    <Host Name="Presentation"/>
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://aliciathornber.github.io/magic-rectangle/taskpane/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <!-- No VersionOverrides block needed unless youâ€™re adding ribbon commands -->
</OfficeApp>
