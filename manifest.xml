<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp"
           Id="c9c94a5f-2dbe-4e7b-9f77-rcorner-lock-001"
           Version="1.0.0.0"
           ProviderName="Rounded Corner Lock"
           DefaultLocale="en-US"
           DisplayName="Rounded Corner Lock"
           Description="Set a rounded rectangle corner radius in px and keep it fixed after resize."
           SupportUrl="https://github.com/<your-username>/rounded-corner-lock">
  <Hosts>
    <Host Name="PowerPoint"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="PowerPointApi" MinVersion="1.10"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation Default="https://<your-username>.github.io/rounded-corner-lock/taskpane/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <IconUrl Default="https://<your-username>.github.io/rounded-corner-lock/assets/icon-32.png"/>
</OfficeApp>
